<?php

namespace Admin\AdminBundle\Entity;

use Doctrine\ORM\EntityRepository;

/**
 * ExperienciasRepository
 *
 * This class was generated by the Doctrine ORM. Add your own custom
 * repository methods below.
 */
class ExperienciasRepository extends EntityRepository
{
    public function findExp()
    {
        $em = $this->getEntityManager();
        $consulta = $em->createQuery('SELECT o.titulo, o.contenido FROM AdminBundle:Experiencias o');
        $resultado = $consulta->getArrayResult();
        return $resultado;
    }
}
